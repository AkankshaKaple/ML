 32, 34, 20, 25)\n","menStd = (2, 3, 4, 1, 2)\n","womenStd = (3, 5, 2, 3, 3)\n","ind = np.arange(N)    # the x locations for the groups\n","width = 0.35       # the width of the bars: can also be len(x) sequence\n","\n","p1 = plt.bar(ind, menMeans, width, yerr=menStd)\n","p2 = plt.bar(ind, womenMeans, width,\n","             bottom=menMeans, yerr=womenStd)\n","\n","plt.ylabel('Scores')\n","plt.xticks(ind, ('G1', 'G2', 'G3', 'G4', 'G5'))\n","plt.yticks(np.arange(0, 81, 10))\n","plt.legend((p1[0], p2[0]), ('Men', 'Women'))\n","\n","plt.show()"],"execution_count":0,"outputs":[{"output_type":"display_data","data":{"image/png":"iVBORw0KGgoAAAANSUhEUgAAAe0AAAFOCAYAAACrPEW7AAAABHNCSVQICAgIfAhkiAAAAAlwSFlz\nAAALEgAACxIB0t1+/AAAADl0RVh0U29mdHdhcmUAbWF0cGxvdGxpYiB2ZXJzaW9uIDIuMS4yLCBo\ndHRwOi8vbWF0cGxvdGxpYi5vcmcvNQv5yAAAHv9JREFUeJzt3XtY1GX+//HXwEAIouE4uNF+g9Y1\nNVHz1DdUtpBqNVuzdlOXC90Omq3iutdlYVGLmodSyytdNCvL9iotXFqTjrDW6m4u4Zpnc9dyrdSU\nRgJPMCYwvz/67ax+jRGRz3y4h+fjL+YzM5/7zRsuXtz3Z+Yeh8/n8wkAADR7YXYXAAAAGobQBgDA\nEIQ2AACGILQBADAEoQ0AgCEIbQAADOG06sQnT57U1KlTdfToUZ0+fVoTJ06U2+3W9OnTJUmdO3fW\njBkzrBoeAICQY1lor169WldeeaWmTJmisrIy/epXv5Lb7VZOTo569OihKVOmaP369br++uutKgEA\ngJBi2fJ4XFycKisrJUnHjh3TpZdeqoMHD6pHjx6SpLS0NJWUlFg1PAAAIcey0B46dKi++uor3XTT\nTcrMzFR2drbatGnjv9/lcsnj8Vg1PAAAIcey5fE1a9YoISFBL7zwgv75z39q4sSJio2N9d/fkN1T\na2pq5XSGW1UiAABGsSy0N2/erIEDB0qSunTpolOnTqmmpsZ/f1lZmeLj4wOeo6KiyqryGs3tjpXH\nc9zuMloUem4P+h589Dz4mmPP3e7Yeu+zbHk8MTFR27ZtkyQdPHhQMTEx6tixozZt2iRJKi4uVmpq\nqlXDAwAQciybaY8cOVI5OTnKzMxUTU2Npk+fLrfbrdzcXNXV1alnz57q37+/VcMDABByLAvtmJgY\nLVy48JzjK1eutGpIAABCGjuiAQBgCEIbAABDENoAABiC0AYAwBCENgAg5B069JUGDuyrnTt3nHX8\n5z//uWbPnm5PUY1g2avHAQD4Pvc88UGTnu/FhwY16HEJCZdr7doiJSd3lyQdOLBfx44da9JarEZo\nAwBahG7dumvTplLV1tYqPDxca9cWacCAAaqsPK5t27bo2WcXy+l0Kj6+g6ZOfVQ7dmzTn/60Sg5H\nmL74Yp9uuCFd99xzn63fA8vjAIAWwel06uqrk7V583c7c3744V/9Hw/99NPz9cQTT2nRoqVq166d\n/vKXtZKkTz7ZpUcema6lS5fr9dfzbav9P5hpAwBajLS0dK1dWySXyyW3263o6Gh98803OnBgv3Jy\nHpQkeb1etW17qdq3d6tz5y6Kioqyuer/IrQBAC1G377